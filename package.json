{
  "name": "@thinknimble/tn-forms",
  "version": "2.1.0",
  "description": "Utilities for building front-end forms.",
  "author": "William Huster <william@thinknimble.com>, Pari Baker <pari@thinknimble.com>",
  "license": "MIT",
  "main": "lib/index.js",
  "scripts": {
    "compile": "babel src -d lib",
    "prepublish": "npm run compile",
    "test": "mocha --require @babel/register",
    "build": "webpack --config webpack.config.js",
    "test-ts": "ts-mocha  -- --verbose=true -p tsconfig.json --require @babel/register test/**.ts"
  },
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thinknimble/tn-forms.git"
  },
  "keywords": [
    "javascript"
  ],
  "homepage": "https://github.com/thinknimble/tn-forms#readme",
  "dependencies": {
    "@thinknimble/tn-utils": "^1.4.0",
    "@types/luxon": "^3.0.0",
    "babel-loader": "^8.2.2",
    "email-validator": "^2.0.4",
    "luxon": "^2.4.0",
    "user": "0.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/preset-env": "^7.9.5",
    "@babel/register": "^7.9.0",
    "@types/expect": "^24.3.0",
    "@types/mocha": "^9.1.1",
    "babel-eslint": "^10.1.0",
    "eslint": "^8.2.0",
    "eslint-config-prettier": "^8.3.0",
    "mocha": "^7.1.1",
    "prettier": "^2.4.1",
    "source-map-loader": "^4.0.0",
    "ts-loader": "^9.3.1",
    "ts-mocha": "^10.0.0",
    "typescript": "^4.7.4",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.10.0"
  },
  "babel": {
    "plugins": [
      "@babel/plugin-proposal-class-properties"
    ],
    "presets": [
      [
        "@babel/preset-env",
        {
          "useBuiltIns": false
        }
      ]
    ]
  }
}
